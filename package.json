{
  "name": "transcript-speaker-tagger",
  "version": "1.0.0",
  "description": "React app for tagging speakers in transcripts",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "http-proxy-middleware": "^2.0.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:backend": "python server.py || python3 server.py",
    "start:dev": "concurrently --kill-others \"npm run start:backend\" \"npm run start:frontend:delayed\"",
    "start:frontend:delayed": "node -e \"setTimeout(() => {}, 3000)\" && npm start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

